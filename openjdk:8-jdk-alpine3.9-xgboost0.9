FROM openjdk:8-jdk-alpine3.9
RUN apk update && \
    apk --no-cache add g++ build-base gcc musl-dev \
    openblas lapack-dev libexecinfo-dev libstdc++ libgomp \
    gcompat \
    abuild \
    binutils \
    binutils-doc \
    gcc-doc \
    libc6-compat \
    libgcc \
    libgomp \
    bash

EXPOSE 8080
